  "scripts": {
    "dev-cy": "yarn cy-install && npx concurrently \"vite --host\" \"yarn coverage-open\"",
    "dev": "./scripts/dev.sh",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean-dev": "rm -rf node_modules/ && yarn && yarn dev",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "sb": "yarn storybook",
    "build-storybook": "storybook build",
    "cy-install": "cypress install",
    "cy:open-e2e": "cypress open --e2e --browser electron",
    "cy:open-unit": "cypress open --component --browser electron",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "coverage-open": "yarn run cy:run-unit && npx serve coverage/lcov-report -p 3321"
  },

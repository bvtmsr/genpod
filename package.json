{
	"name": "intelops-genpod",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "npx concurrently \"vite --host\" \"yarn coverage-open\"",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"storybook": "storybook dev -p 6006",
		"sb": "yarn storybook",
		"build-storybook": "storybook build",
		"cy:open-e2e": "cypress open --e2e --browser electron",
		"cy:open-unit": "cypress open --component --browser electron",
		"cy:run-e2e": "cypress run --e2e",
		"cy:run-unit": "cypress run --component",
		"coverage-open": "yarn run cy:run-unit && npx serve coverage/lcov-report -p 3321"
	},
